{
  "name": "spotify-extension",
  "version": "1.0.0",
  "description": "Spotify Player Web extension",
  "main": "background.js",
  "scripts": {
    "prettier": "prettier --write '**/**/*.{ts,tsx}'",
    "chrome:dev": "env BROWSER='chrome' webpack --config webpack/webpack.dev.js --watch",
    "firefox:dev": "env BROWSER='firefox' webpack --config webpack/webpack.dev.js --watch",
    "edge:dev": "env BROWSER='edge' webpack --config webpack/webpack.dev.js --watch",
    "chrome": "rimraf dist && env BROWSER='chrome' webpack --config webpack/webpack.prod.js",
    "firefox": "rimraf dist && env BROWSER='firefox' webpack --config webpack/webpack.prod.js",
    "edge": "rimraf dist && env BROWSER='edge' webpack --config webpack/webpack.prod.js",
    "typecheck": "./node_modules/.bin/tsc --noEmit",
    "test": "jest",
    "lint": "eslint '**/**/*.{ts,tsx}'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidnguyen179/spotify-extension.git"
  },
  "keywords": [
    "spotify",
    "spotify web api",
    "spotify player",
    "chrome extension",
    "spotify chrome extension",
    "spotify microsoft edge extension",
    "spotify firefox addon",
    "web extension",
    "typescript",
    "ts",
    "jest",
    "sinon"
  ],
  "author": "David Nguyen <dzungnguyen179@gmail.com> (dzungnguyen179@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidnguyen179/spotify-extension/issues"
  },
  "homepage": "https://github.com/davidnguyen179/spotify-extension#readme",
  "devDependencies": {
    "@babel/preset-env": "7.12.11",
    "@babel/preset-typescript": "7.12.7",
    "@types/chrome": "0.0.129",
    "@types/jest": "26.0.20",
    "@types/sinon": "9.0.10",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "babel-jest": "26.6.3",
    "copy-webpack-plugin": "7.0.0",
    "eslint": "7.21.0",
    "eslint-plugin-import": "2.22.1",
    "git-hooks-plus": "1.0.1",
    "html-webpack-plugin": "4.5.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "sinon": "9.2.4",
    "terser-webpack-plugin": "5.1.1",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.14",
    "typescript": "4.1.3",
    "web-ext-types": "3.2.1",
    "webpack": "5.18.0",
    "webpack-cli": "4.4.0",
    "webpack-merge": "5.7.3",
    "whatwg-fetch": "3.0.0"
  },
  "dependencies": {
    "colorthief": "2.3.2"
  }
}
